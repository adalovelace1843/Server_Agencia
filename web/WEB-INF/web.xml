<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	 version="3.0">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <context-param>
        <param-name>agencia</param-name>
        <param-value>Abitab</param-value>
    </context-param>
    <servlet>
        <servlet-name>ServletServidor</servlet-name>
        <servlet-class>serverAgencia.ServletServidor</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
     <servlet-mapping>
        <servlet-name>ServletServidor</servlet-name>
        <url-pattern>/ServletServidor</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServletIngresoUsuarios</servlet-name>
        <servlet-class>servlets.ServletIngresoUsuarios</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
     <servlet-mapping>
        <servlet-name>ServletIngresoUsuarios</servlet-name>
        <url-pattern>/ServletIngresoUsuarios</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServletLogin</servlet-name>
        <servlet-class>servlets.ServletLogin</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
     <servlet-mapping>
        <servlet-name>ServletLogin</servlet-name>
        <url-pattern>/ServletLogin</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServletMenu</servlet-name>
        <servlet-class>servlets.ServletMenu</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
     <servlet-mapping>
        <servlet-name>ServletMenu</servlet-name>
        <url-pattern>/ServletMenu</url-pattern>
    </servlet-mapping>
</web-app>
